{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories" :[
    {
      "type": "vcs",
      "url": "https://github.com/jaffle-be/modules"
    }
  ],
  "require": {
    "laravel/framework": "~5.1",
    "predis/predis": "~1.0",
    "guzzlehttp/guzzle": "~5.0",
    "intervention/image": "~2.2",
    "fzaninotto/faker": "~1.4",
    "intervention/imagecache": "~2.2",
    "mews/captcha": "^2.0",
    "thomaswelton/laravel-gravatar": "~1.0",
    "davejamesmiller/laravel-breadcrumbs": "~3.0",
    "elasticsearch/elasticsearch": "~2.0",
    "intouch/laravel-newrelic": "~2.0",
    "barryvdh/laravel-debugbar": "~2.0",
    "cviebrock/eloquent-sluggable": "dev-master",
    "graham-campbell/markdown": "~5.0",
    "webuni/commonmark-attributes-extension": "~0.3.0",
    "thujohn/twitter": "^2.0",
    "alaouy/Youtube": "*",
    "vinkla/vimeo": "^3.0",
    "pusher/pusher-php-server": "~2.0",
    "fideloper/proxy": "~3.0",
    "pingpong/modules": "dev-master",
    "laravelrus/localized-carbon": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "modules/",
      "Themes\\": "themes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
